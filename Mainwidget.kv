                        color: 0,0,0,1
                    Label:
                        id: querosine
                        text: ' -.-ºC'
                        font_size: 25 
                        bold: True
                        halign: 'center'
                        pos_hint:{'x':0.11, 'y':-0.01}
                        color: 0,0,0,1
                    Label:
                        id: diesel
                        text: ' -.-ºC'
                        font_size: 25 
                        bold: True
